			<gui text="66" title="159" src="editcopy.png" accessKey="copy_access_key" hasAccessKey="true">
				<context selection="true" dir="" recycle="hidden" actionBar="true" contextMenu="true" infoPanel="false" actionBarGroup="change">
				</context>
